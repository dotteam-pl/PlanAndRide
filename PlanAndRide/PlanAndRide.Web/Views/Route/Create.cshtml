@model PlanAndRide.Web.Models.RouteViewModel

@{
    ViewData["Title"] = "Create";
    var apiKey = ViewData["ApiKey"];
}

<h1>Create</h1>

<h4>Route</h4>
<hr />
<div class="row">
    <div class="col-lg-8">
        <form asp-action="Create">
            <partial name="_Partials/_CreateEditForm" model="@Model" />
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>


@section Scripts {
    @{
    await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }

<script type="text/javascript" src='https://maps.googleapis.com/maps/api/js?libraries=places&key=@apiKey'></script>
<script type="text/javascript">
    window.addEventListener('load', AutocomplePlaces);
</script>
}
